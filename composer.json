  "description": "JobQueue is a PHP component that queues `Tasks` defining `Jobs` which can be executed by `Workers`",
  "homepage": "https://github.com/bricev/JobQueue",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "monolog/monolog": "^1.0",
    "predis/predis": "^1.0",
    "ramsey/uuid": "^3.0",
    "symfony/config": "^4.0",
    "symfony/console": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "JobQueue\\": "src"
    }
  },
  "bin": [
    "bin/jobqueue"
  ]
}
